pipeline {
   agent { label 'ec2-fleet'}

   stages {
      stage('Run Docker') {
         steps {
            sh 'docker info'
         }
      }

      stage('Generate Docker ENV') {
          steps {
             sh 'docker run -i shuangyu/jenkins-docker-env bash'
             sh 'pwd'
             sh 'git --version'
           }
       }

     stage('pwd') {
         steps {
             sh 'pwd'
          }
      }


      stage('Build number') {

         steps {
         echo "Build number is ${currentBuild.number}"
         }
      }


   }
}
