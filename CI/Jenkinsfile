pipeline {
   agent { label 'java-docker-slave'}

   stages {
      stage('Hello') {
         steps {
            echo 'Hello World'
         }
      }

     stage('pwd') {
         steps {
             sh 'pwd'
          }
      }


      stage('Build number') {

         steps {
         echo "Build number is ${currentBuild.number}"
         }
      }


   }
}
